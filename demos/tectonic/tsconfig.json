{
  "compilerOptions": {
    "experimentalDecorators": true,
    "inlineSourceMap": false,
    "module": "amd",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "jsx": "react",
    "jsxFactory": "tsx",
    "target": "es5",
    "lib": [
      "dom",
      "es2015.promise",
      "scripthost",
      "es5"
    ]
  },
  "include": [
    "js/**/*",
    "typings/**/*"
  ]
}
